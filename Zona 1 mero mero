       mp = MPU();
     }
     detenerselento();
  }
  
   else if(mp > mp3)
   {
      while(mp > mp3)
     {
        analogWrite(DerechaAdelante, LOW);  
        analogWrite(DerechaAtras, (VelDerecha*.4));  
        analogWrite(IzquierdaAdelante, (VelIzquierda*.4));  
        analogWrite(IzquierdaAtras, LOW); 
        mp = MPU();
     }
     detenerselento();
   }
}
