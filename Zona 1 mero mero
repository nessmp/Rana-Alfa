 detenerselento();
 adelante();
 delay(500);
 detenerselento();
 delay(500);

 mp = MPU();//SEGUNDA
 mp2= mp + 90;
 if(mp2 > 359)
 {
  mp2 -= 360;
 }
 mp3 = mp + 98;
 if(mp3 > 359)
 {
  mp3 -= 360;
 }
 
